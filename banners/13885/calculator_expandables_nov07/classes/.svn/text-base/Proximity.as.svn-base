class classes.Proximity{	function Proximity()	{			}		// pass the object to affect and the mouse scope interacting with the object	static function getProximity(_obj:MovieClip, targetClip:MovieClip):Number	{		var xPos:Number = _obj._x;		var yPos:Number = _obj._y;		var clipX:Number = targetClip._x;		var clipY:Number = targetClip._y;				var prox:Number = Math.sqrt((xPos - clipX) * (xPos - clipX) + (yPos - clipY) * (yPos - clipY));		if(prox < 100)		{			// return percentage			return 100 - prox;		}		else		{			// return nada			return 0;		}	}}