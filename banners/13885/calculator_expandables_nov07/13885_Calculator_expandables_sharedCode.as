// 13885_Calculator_expandables_sharedCode.as//import classes.Tween;import mx.transitions.Tween;import mx.transitions.easing.*;import flash.filters.BlurFilter;// variables  -------------------------------------------------------------------------------------_quality = "BEST";// blur filters for x or y motionvar xMotionBlur:BlurFilter = new BlurFilter(8, 0, 2);var yMotionBlur:BlurFilter = new BlurFilter(0, 8, 2);var startScale:Number = 100;var endScale:Number = 120;// functions  -------------------------------------------------------------------------------------/* examples:_clip:MovieClip -------- mcT1_destination:Number ---- 342_duration:Number ------- .6_axis:String ----------- "x" or "y"*/function blurSlideObj(_clip:MovieClip, _offset:Number, _duration:Number, _axis:String):Void{	if(_axis == "x")	{		_clip.filters = [xMotionBlur];		var xt:Tween = new Tween(_clip, "_x", Strong.easeOut, _offset, _clip._x, _duration, true);		xt.onMotionFinished = function():Void		{			_clip.filters = [];		}	}	else	{		_clip.filters = [yMotionBlur];		var yt:Tween = new Tween(_clip, "_y", Strong.easeOut, _offset, _clip._y, _duration, true);		yt.onMotionFinished = function():Void		{			_clip.filters = [];		}	}}function fadeTween(_clip:MovieClip, a1:Number, a2:Number, _duration:Number):Void{	new Tween(_clip, "_alpha", Strong.easeOut, a1, a2, _duration, true);}function proxAutomatically():Void{	var proxTween:Tween = new Tween(mcT8.mcProx, "_x", Strong.easeOut, -150, 400, 1.5, true);}function scaleMCT9_sub():Void{	mcT9.mcT9_sub._alpha = 100;	new Tween(mcT9.mcT9_sub, "_xscale", Strong.easeOut, 100, 300, 3, true);	new Tween(mcT9.mcT9_sub, "_yscale", Strong.easeOut, 100, 300, 3, true);	new Tween(mcT9.mcT9_sub, "_alpha", Strong.easeOut, 100, 0, 3, true);}function ctaClideAnim():Void{	mcCTA.gotoAndPlay("slide");}