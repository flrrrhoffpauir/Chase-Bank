// CH_DE_DR_backup_Sep07.asimport classes.transitions.Tweens;// variables  -------------------------------------------------------------------------------------_quality = "BEST";var replayInt:Number;var _tweens:Tweens = new Tweens();var ctaStartScale:Number = 100;var ctaEndScale:Number = 105;// functions  -------------------------------------------------------------------------------------function defaultClick():Void{	getURL(_root.clickTag, "_blank");}function objOnStageTween(clip:MovieClip, theDirection:String, newStartPos:Number):Void{	var prop:String;	switch(theDirection)	{		case "left": prop = "_x"; var startPos:Number = clip._x - newStartPos; break;		case "right": prop = "_x"; var startPos:Number = clip._x + newStartPos; break;		case "top": prop = "_y"; var startPos:Number = clip._y - newStartPos; break;		case "bottom": prop = "_y"; var startPos:Number = clip._y + newStartPos; break;		// fade in effect only		case "none": var startPos:Number = clip._x; break;	}	if(prop == "_x")	{		_tweens.xBackTween(clip, startPos, clip[prop], 1);	}	else if(prop == "_y")	{		_tweens.yBackTween(clip, startPos, clip[prop], 1);	}	_tweens.fadeTween(clip, 0, 100, .5);}function creditCardTween(theDirection:String, startPosOffset:Number):Void{	if(theDirection == "top")	{		_tweens.yTween(creditCard_mc, creditCard_mc._y - startPosOffset, creditCard_mc._y, 1);	}	else if(theDirection == "left")	{		_tweens.xTween(creditCard_mc, creditCard_mc._x - startPosOffset, creditCard_mc._x, 1);	}	else	{		_tweens.xTween(creditCard_mc, creditCard_mc._x + startPosOffset, creditCard_mc._x, 1);	}	_tweens.fadeTween(creditCard_mc, 0, 100, .5);}// this is for the 728 & 468 onlyfunction creditCardTween2(startPos:Number, endPos:Number):Void{	_tweens.xBounceTween2(creditCard_mc, startPos, endPos, .4, cta_mc.learnMore_mc);	_tweens.fadeTween(creditCard_mc, 0, 100, .5);}function showCTA(axis:String, startPosOffset:Number):Void{	cta_mc.onRollOver = function():Void	{		_tweens.scaleTween(this.learnMore_mc, ctaStartScale, ctaEndScale, .25);		this.learnMore_mc.gotoAndPlay("shimmer");	}	cta_mc.onRollOut = function():Void	{		_tweens.scaleTween(this.learnMore_mc, ctaEndScale, ctaStartScale, .25);	}	cta_mc.onRelease = defaultClick;		// args:  ctaShimTween(mc:MovieClip, y1:Number, y2:Number, time:Number, _pathToShimmer:MovieClip):Void	_tweens.ctaShimTween(cta_mc, axis, cta_mc[axis] + startPosOffset, cta_mc[axis], .5, cta_mc.learnMore_mc);	}function hideAllObjects():Void{	_tweens.fadeTween(creditCard_mc, 100, 0, .5);	_tweens.fadeTween(headline1_mc, 100, 0, .5);	_tweens.fadeTween(headline2_mc, 100, 0, .5);	_tweens.fadeTween(headline3_mc, 100, 0, .5);	_tweens.fadeTween(headline4_mc, 100, 0, .5);	_tweens.fadeTween(cta_mc, 100, 0, .5);}// handlers  --------------------------------------------------------------------------------------hotSpot_btn.onRelease = defaultClick;// run at once  -----------------------------------------------------------------------------------