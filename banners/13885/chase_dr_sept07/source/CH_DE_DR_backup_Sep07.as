// CH_DE_DR_backup_Sep07.asimport Tweens;// variables  -------------------------------------------------------------------------------------_quality = "BEST";var _tweens:Tweens = new Tweens();var slideSpeed:Number = .4;// functions  -------------------------------------------------------------------------------------function defaultClick():Void{	getURL(_root.clickTAG, "_blank");}function objTween(clip:MovieClip, theDirection:String, newPos:Number):Void{	switch(theDirection)	{		case "on":			_tweens.xTween(clip, clip._x - newPos, clip._x, slideSpeed);			break;		case "off":			_tweens.xTween(clip, clip._x, clip._x + newPos, slideSpeed);			break;	}}function fadeInOut(clip:MovieClip, theDirection:String):Void{	switch(theDirection)	{		case "in":			_tweens.fadeTween(clip, 0, 100, .5);			break;		case "out":			_tweens.fadeTween(clip, 100, 0, .5);			break;	}}function showCreditCard():Void{	_tweens.xTween(creditcard_mc, creditcard_mc._x + 300, creditcard_mc._x, 1);}function showCreditCard728():Void{	_tweens.xTween(creditcard_mc, creditcard_mc._x - 500, creditcard_mc._x, 1);}function showCreditCard160():Void{	_tweens.yTween(creditcard_mc, creditcard_mc._y - 500, creditcard_mc._y, 1);	_tweens.fadeTween(cardShadow_mc, 0, 100, 1);}// this is for the 728 & 468 onlyfunction creditCardTween2(startPos:Number, endPos:Number):Void{	_tweens.xBounceTween2(creditCard_mc, startPos, endPos, .4, cta_mc.learnMore_mc);	_tweens.fadeTween(creditCard_mc, 0, 100, .5);}function showCTA():Void{	cta_anim.cta_mc.onRollOver = function():Void	{		this.gotoAndPlay("shimmer");	}	cta_anim.cta_mc.onRelease = defaultClick;		_tweens.yTween(cta_anim.cta_mc, 44, 14, .7);	_tweens.yTween(cta_anim_reflect.ctaReflect_mc, -15, 15, .7);}// handlers  --------------------------------------------------------------------------------------hotspot_mc.onRelease = defaultClick;// run at once  -------------------------------------------------