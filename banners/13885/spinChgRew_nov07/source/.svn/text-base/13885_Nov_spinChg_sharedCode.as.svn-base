// 13885_Nov_spinChg_sharedCode.asimport classes.Tweens;// variables  -------------------------------------------------------------------------------------_quality = "BEST";var ctaStartScale:Number = mcCTA._xscale;var ctaEndScale:Number = mcCTA._xscale + 10;_global._tweens = new Tweens();// functions  -------------------------------------------------------------------------------------function defaultClick():Void {	getURL(_root.clickTag, "_blank");}function showCTA():Void{	mcCTA.onRollOver = function():Void	{		_tweens.scaleTween(this.mcApplyNow, ctaStartScale, ctaEndScale, .25);		this.mcApplyNow.gotoAndPlay("shimmer");	}	mcCTA.onRollOut = function():Void	{		_tweens.scaleTween(this.mcApplyNow, ctaEndScale, ctaStartScale, .25);	}	mcCTA.onRelease = defaultClick;	_tweens.yTween(mcCTA, mcCTA._y + 100, mcCTA._y, .5);	}function showCTA_728():Void{	mcCTA.onRollOver = function():Void	{		_tweens.scaleTween(this.mcApplyNow, ctaStartScale, ctaEndScale, .25);		this.mcApplyNow.gotoAndPlay("shimmer");	}	mcCTA.onRollOut = function():Void	{		_tweens.scaleTween(this.mcApplyNow, ctaEndScale, ctaStartScale, .25);	}	mcCTA.onRelease = defaultClick;	_tweens.xTween(mcCTA, mcCTA._x + 160, mcCTA._x, .5);	}function showCreditCard(_clip:MovieClip):Void{	_tweens.fadeTween(_clip, _clip._alpha, 100, .6);	_tweens.yTween(_clip, _clip._y, _clip._y - 25, 1);}function hideCreditCard(_clip:MovieClip):Void{	_tweens.fadeTween(_clip, _clip._alpha, 0, .6);	_tweens.yTween(_clip, _clip._y, _clip._y + 25, 1);}function moveTextOnstage(_clip:MovieClip, _distanceToMove:Number):Void{	_tweens.xTween(_clip, _clip._x + _distanceToMove, _clip._x, .6);}function moveTextOffstage(_clip:MovieClip, _distanceToMove:Number):Void{	_tweens.xTween(_clip, _clip._x, _clip._x - _distanceToMove, 1.2);}// opposite motionfunction moveTextOnstageR(_clip:MovieClip, _distanceToMove:Number):Void{	_tweens.xTween(_clip, _clip._x - _distanceToMove, _clip._x, .6);}function moveTextOffstageR(_clip:MovieClip, _distanceToMove:Number):Void{	_tweens.xTween(_clip, _clip._x, _clip._x + _distanceToMove, 1.2);}// handlers  --------------------------------------------------------------------------------------btnHotSpot.onRelease = defaultClick;// run at once  -----------------------------------------------------------------------------------