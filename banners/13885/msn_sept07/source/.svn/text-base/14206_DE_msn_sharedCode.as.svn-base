// 14206_DE_msn_sharedCode.asimport Tweens;// variables  -------------------------------------------------------------------------------------_quality = "BEST";var replayInt:Number;var ctaStartScale:Number = 100;var ctaEndScale:Number = 110;_global._tweens = new Tweens();// functions  -------------------------------------------------------------------------------------/* removed for MSN - code on button instancesfunction defaultClick():Void {	getURL(clickTAG, "_blank");}*/function presentHeadline(mc:MovieClip, theDirection:String, distance:Number):Void{	var amt:Number = (theDirection == "left") ? -distance : distance;	_tweens.xTween(mc, mc._x + amt, mc._x, .5);}function shrinkCard():Void{	_tweens.scaleTween(creditCard_mc, 100, 75, .4);	_tweens.xTween(creditCard_mc, creditCard_mc._x, 150, .4);	_tweens.yTween(creditCard_mc, creditCard_mc._y, 150, .4);}function dropFadeAutoText():Void{	_tweens.yTween(headline3_mc, headline3_mc._y - 30, headline3_mc._y, .5);	_tweens.fadeTween(headline3_mc, 0, 100, .5);}function showReplay():Void{	//_tweens.fadeTween(replay_btn, 0, 100, .5);	_tweens.yTween(replay_btn, replay_btn._y + 300, replay_btn._y, .5);	replay_btn.onRelease = cleanUpAndReplay;}function showCTA():Void{	cta_mc.onRollOver = function():Void	{		_tweens.scaleTween(this, ctaStartScale, ctaEndScale, .5);		this.gotoAndPlay("shimmer");	}	cta_mc.onRollOut = function():Void	{		_tweens.scaleTween(this, ctaEndScale, ctaStartScale, .5);	}	//cta_mc.onRelease = defaultClick;	_tweens.yTween(cta_mc, cta_mc._y + 100, cta_mc._y, .5);	}function cleanUpAndReplay():Void{	play();	_tweens.fadeTween(headline1_mc, 100, 0, .5);	_tweens.fadeTween(headline2_mc, 100, 0, .5);	_tweens.fadeTween(headline3_mc, 100, 0, .5);	_tweens.fadeTween(creditCard_mc, 100, 0, .5);	_tweens.fadeTween(cta_mc, 100, 0, .5);}// handlers  --------------------------------------------------------------------------------------//hotSpot_btn.onRelease = defaultClick;// run at once  -----------------------------------------------------------------------------------