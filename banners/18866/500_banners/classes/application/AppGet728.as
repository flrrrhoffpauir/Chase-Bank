import application.AppGet;   import gs.TweenLite;import gs.easing.*; import gs.plugins.*;class application.AppGet728 extends AppGet {               			public function AppGet728(mc:MovieClip, o:Object) {		super(mc, o);	}                  		public function playIntro():Void {		_stage.copy1_mc._alpha = 100;		_stage.copy2_mc._alpha = 100;		_stage.copy3_mc._alpha = 100;								TweenLite.from(_stage.copy1_mc, 1, {_x:"-20", _alpha:0, delay:0.5, ease:Quad.easeOut, overwrite:3});		TweenLite.from(_stage.copy2_mc, 1, {_x:"20", _alpha:0, delay:1.3, ease:Quad.easeOut, overwrite:3});		TweenLite.from(_stage.copy3_mc, 1, {_x:"20", _alpha:0, delay:1.5, ease:Quad.easeOut, overwrite:3});		TweenLite.to(null, 0, {delay:3, onComplete:setupCard, onCompleteScope:this, overwrite:3});	}      		private function playFinal():Void {         				TweenLite.to(_stage.copy1_mc, .4, {_alpha:0, delay:0.4, ease:Quad.easeOut, overwrite:3});		TweenLite.to(_stage.copy2_mc, .4, {_alpha:0, delay:0.4, ease:Quad.easeOut, overwrite:3});		TweenLite.to(_stage.copy3_mc, .4, {_alpha:0, delay:0.4, ease:Quad.easeOut, overwrite:3});						TweenLite.to(_stage.finalFrame_mc, .4, {_alpha:100, delay:0.5, ease:Quad.easeOut, overwrite:3})				TweenLite.from(_stage.finalFrame_mc.bullet1_mc, .7, {_alpha:0, _x:"10", delay:1.5, ease:Quad.easeOut, overwrite:3});		TweenLite.from(_stage.finalFrame_mc.bullet2_mc, .7, {_alpha:0, _x:"10", delay:2.0, ease:Quad.easeOut, overwrite:3});		TweenLite.from(_stage.finalFrame_mc.bullet3_mc, .7, {_alpha:0, _x:"10", delay:2.5, ease:Quad.easeOut, overwrite:3});   				}	        	}